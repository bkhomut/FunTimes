<h1>Editing Note #<%= @note.id %></h1>


<%= form_tag(note_url(@note), method: 'put') do %>

  <div>
    <%= label_tag :engagement_id %><br />
    <%= number_field_tag :engagement_id, @note.engagement_id %>
  </div>

  <div>
    <%= label_tag :note %><br />
    <%= text_area_tag :note, @note.note %>
  </div>

  
  <div>
    <%= submit_tag "Update Note" %>
  </div>

<% end %>
