<h1>New Note</h1>


<%= form_tag(notes_url, method: 'post') do %>


  <div>
    <%= label_tag :engagement_id %><br />
    <%= number_field_tag :engagement_id %>
  </div>

  <div>
    <%= label_tag :note %><br />
    <%= text_area_tag :note %>
  </div>


  <div>
    <%= submit_tag "Create Note" %>
  </div>
<% end %>
