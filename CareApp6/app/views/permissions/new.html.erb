<h1>New Permission</h1>


<%= form_tag(permissions_url, method: 'post') do %>


  <div>
    <%= label_tag :engagement_id %><br />
    <%= number_field_tag :engagement_id %>
  </div>

  <div>
    <%= label_tag :user_id %><br />
    <%= number_field_tag :user_id %>
  </div>


  <div>
    <%= submit_tag "Create Permission" %>
  </div>
<% end %>
