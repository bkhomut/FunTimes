<h1>Editing Note #<%= @note.id %></h1>


<%= form_tag(note_url(@note), method: 'put') do %>

  <div>
    <%= label_tag :note_id %><br />
    <%= text_field_tag :note_id, @note.note_id %>
  </div>

  <div>
    <%= label_tag :case_id %><br />
    <%= text_field_tag :case_id, @note.case_id %>
  </div>

  <div>
    <%= label_tag :note %><br />
    <%= text_area_tag :note, @note.note %>
  </div>

  
  <div>
    <%= submit_tag "Update Note" %>
  </div>

<% end %>
