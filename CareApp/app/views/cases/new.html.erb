<h1>New Case</h1>


<%= form_tag(cases_url, method: 'post') do %>


  <div>
    <%= label_tag :case_id %><br />
    <%= text_field_tag :case_id %>
  </div>

  <div>
    <%= label_tag :user_id %><br />
    <%= text_field_tag :user_id %>
  </div>

  <div>
    <%= label_tag :condition_id %><br />
    <%= text_field_tag :condition_id %>
  </div>


  <div>
    <%= submit_tag "Create Case" %>
  </div>
<% end %>
